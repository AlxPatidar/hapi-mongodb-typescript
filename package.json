    "hapi-auth-jwt2": "8.1.0",
    "hapi-swagger": "9.1.1",
    "inert": "5.1.0",
    "joi": "13.2.0",
    "jsonwebtoken": "8.2.1",
    "lodash": "4.17.15",
    "mongoose": "5.7.5",
    "nconf": "0.10.0",
    "nodemon": "1.17.3",
    "vision": "5.3.2"
  },
  "devDependencies": {
    "@types/node": "9.4.7",
    "node": "9.8.0",
