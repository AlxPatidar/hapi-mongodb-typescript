{
  "name": "hapi-typescript",
  "version": "1.3.1",
  "description": "Hapi-typescript-mongodb",
  "license": "MIT",
  "author": "Ashok Patidar",
  "email": "ashokpatidar80@gmail.com",
  "keywords": [
    "typescript",
    "project structure",
    "nodejs"
  ],
  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
    "start": "tsc && node build/index.js",
    "start:dev": "nodemon",
    "setup": "npm install",
    "watch": "nodemon --watch build/src build/src/index.js"
  },
    "nodemonConfig": {
      "ignore": [
        "**/*.test.ts",
        "**/*.spec.ts",
        ".git",
        "node_modules"
      ],
      "watch": [
        "src"
      ],
      "exec": "npm start",
      "ext": "ts"
    },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "boom": "7.2.0",
    "dotenv": "^8.1.0",
    "eslint": "4.19.0",
    "fs": "0.0.1-security",
    "good": "8.1.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.0.2",
    "hapi": "17.4.0",
    "hapi-auth-jwt2": "8.1.0",
    "hapi-swagger": "9.1.1",
    "inert": "5.1.0",
    "joi": "13.2.0",
    "jsonwebtoken": "8.2.1",
    "moment": "2.22.1",
    "moment-timezone": "0.5.16",
    "mongoose": "5.0.16",
    "nconf": "0.10.0",
    "nodemon": "1.17.3",
    "path": "0.12.7",
    "rotating-file-stream": "1.3.6",
    "vision": "5.3.2"
  },
  "devDependencies": {
    "@types/node": "9.4.7",
    "mocha": "^5.0.0",
    "node": "9.8.0",
    "sinon": "4.4.6",
    "tslint": "5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": "8"
  }
}
